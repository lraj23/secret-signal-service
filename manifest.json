{
	"display_information": {
		"name": "Secret Signal Service"
	},
	"features": {
		"app_home": {
			"home_tab_enabled": false,
			"messages_tab_enabled": true,
			"messages_tab_read_only_enabled": false
		},
		"bot_user": {
			"display_name": "Secret Signal Service",
			"always_online": true
		}
	},
	"oauth_config": {
		"scopes": {
			"bot": [
				"channels:read",
				"chat:write",
				"chat:write.public",
				"app_mentions:read",
				"im:history",
				"channels:history",
				"reactions:write"
			]
		}
	},
	"settings": {
		"event_subscriptions": {
			"bot_events": [
				"message.channels",
				"message.im",
				"app_mention"
			]
		},
		"interactivity": {
			"is_enabled": true
		},
		"org_deploy_enabled": true,
		"socket_mode_enabled": true,
		"token_rotation_enabled": false
	}
}